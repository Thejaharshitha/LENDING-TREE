<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site2.css")" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
</head>
<body>
    @Html.DisplayName("LENDING TREE APPLICATION")
    <ul id="menu">
        <li>

            @if (Request.IsAuthenticated)
            {

                @Html.ActionLink("Log Out", "Logout", "Account")

            }
            else
            {
                @Html.ActionLink("Register", "Register", "Account")
                @Html.ActionLink("Login", "Login", "Account")
                @Html.ActionLink("AgentRegister", "AgentRegister", "AgentAccount")
                @Html.ActionLink("Agent Login", "AgentLogin", "AgentAccount")
                @Html.ActionLink("Approval Agency", "ApprovalAgency", "AgentAccount")
                @Html.ActionLink("Loan Agency", "LoanAgencyAdmin", "AgentAccount")

            }
        </li>
    </ul>
    <section id="main">
        <div class="container body-content">

            @RenderBody()
        </div>
    </section>
    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year - LENDING TREE APPLICATION</p>
    </footer>
</body>
</html>